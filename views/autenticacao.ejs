<%-include('./partials/header.ejs')%>


<main>
    
    <div class="container">
     
        <form action="/autenticacao"  method="post" class="signup-form">
            <h1>Cadastre-se</h1>
            <label for="nome"><b>Nome</b></label>
            <input type="text" placeholder="Digite seu nome completo" id="nome" name="nome" required>
            <label for="telefone"><b>Telefone</b></label>
            <input type="text" placeholder="Digite seu nome completo" id="telefone" name="telefone" required>
            <label for="endereco"><b>Endereco</b></label>
            <input type="text" placeholder="Digite seu nome completo" id="endereco" name="endereco" required>
            <label for="email"><b>Email</b></label>
            <input type="email" placeholder="Digite seu endereço de email" id="email" name="email" required>
            <label for="username"><b>Novo Usuário</b></label>
            <input type="text" placeholder="Digite um nome de usuário" id="username" name="username" required>
            <label for="senha"><b>Nova Senha</b></label>
            <input type="password" placeholder="Digite uma senha" id="senha" name="senha" required>
            <button type="submit">Cadastrar</button>
        </form>
    </div>

</main>


<%-include('./partials/footer.ejs')%>